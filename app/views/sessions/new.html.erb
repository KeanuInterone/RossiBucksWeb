<p id="alert"><%= alert %></p>
<h1>Log in</h1>
<%= form_tag create_session_path do %>
    <div class="field">
        <%= label_tag :email %>
        <%= email_field_tag :email %>
    </div>
    <div class="field">
        <%= label_tag :password %>
        <%= password_field_tag :password %>
    </div>
    <div class="action">
        <%= submit_tag 'Log In'%>
    </div>
<% end %>
<br>
<br>
<div class="action">
    <%= link_to 'Create Account', new_user_path %>
</div>
